modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "gobject-introspection", "gtk3", "lcms2", "libGL", "libGLU", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "pkg-config", "qhull", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Face Mask Detection System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "face_mask_detection"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Face Mask Detection"

[[workflows.workflow]]
name = "Face Mask Detection System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install opencv-python tensorflow pillow numpy matplotlib && python main.py"

[[workflows.workflow]]
name = "face_mask_detection"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install opencv-python tensorflow pillow numpy matplotlib && python main.py"

[[workflows.workflow]]
name = "Web Face Mask Detection"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python web_app.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80
